<section  class="py-6 contact_form_general_infomation webform_bundle">

  <div class="container">
      <section class="text-center">
        {%  if true in content.field_webform_intro_header[0] %}
          <h1>{{-content.field_webform_intro_header[0]-}}</h1>
        {% endif %}

          {%  if true in content.field_webform_intro_desc[0] %}
          <p class="lead">{{-content.field_webform_intro_desc[0]-}}</p>
          {% endif %}
      </section> 
      {%  if "Off" in content.field_webform_hide_top_section[0] %}

    <div class="row">
      <div class="col-md-4 text-center ">
        <svg class="svg-icon svg-icon-light text-primary w-3rem h-3rem mb-3">
          <use xlink:href="#navigation-map-1">
            <svg viewBox="0 0 64 64" xmlns="http://www.w3.org/2000/svg" id="navigation-map-1" width="100%" height="100%">
              <title>Navigation Map</title>
              <desc>A line styled icon from Orion Icon Library.</desc>
              <path d="M24.4 32.8L20 33.9l-18-6v28L20 62l24-6.1 18 5v-27l-12-4m-30 4V62m24-24v17.9" stroke-miterlimit="10" stroke="#202020" fill="none" data-name="layer2" stroke-linejoin="round" stroke-linecap="round" style="stroke:var(--layer1, #202020)"></path>
              <path stroke-miterlimit="10" stroke="#202020" fill="none" d="M32 2a14 14 0 0 0-14 14c0 12.6 14 26 14 26s14-13.4 14-26A14 14 0 0 0 32 2z" data-name="layer1" stroke-linejoin="round" stroke-linecap="round" style="stroke:var(--layer1, #202020)"></path>
              <circle stroke-miterlimit="10" stroke="#202020" fill="none" r="4" cy="16" cx="32" data-name="layer1" stroke-linejoin="round" stroke-linecap="round" style="stroke:var(--layer1, #202020)"></circle>
            </svg>
          </use>
        </svg>
        <h4 class="ff-base">{% spaceless %}{{-content.field_webform_address_sec_header[0]-}}{% endspaceless %}</h4>
        <p class="text-muted">{% spaceless %}{{-content.field_webform_address_sec_desc[0]-}}{% endspaceless %}</p>
        <p class="text-muted">{% spaceless %}{{-content.field_webform_address[0]-}}{% endspaceless %}</p>
      </div>
      <div class="col-md-4 text-center">
        <svg class="svg-icon svg-icon-light text-primary w-3rem h-3rem mb-3">
          <use xlink:href="#audio-call-1">
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 64 64" id="audio-call-1" width="100%" height="100%">
              <title>Audio Call</title>
              <desc>A line styled icon from Orion Icon Library.</desc>
              <path data-name="layer2" d="M32 62a30 30 0 1 1 28.9-22M32 62c15.5 0 22.8-9.3 16.7-15.4" fill="none" stroke="#202020" stroke-miterlimit="10" stroke-linejoin="round" stroke-linecap="round" style="stroke:var(--layer1, #202020)"></path>
              <path data-name="layer1" d="M49.1 40c.8.5 2.1 1.3 1.8 3a8.6 8.6 0 0 1-8.1 6c-5.4 0-12-4.2-17.8-10.1S15 26.5 15 21.1a8.6 8.6 0 0 1 6-8.1c1.7-.2 2.4 1 3 1.8l4.1 6.2a2.9 2.9 0 0 1-.8 3.9c-1.7 1.4-3.6 2.2 2.9 8.7s7.3 4.7 8.7 2.9a2.9 2.9 0 0 1 3.9-.8z" fill="none" stroke="#202020" stroke-miterlimit="10" stroke-linejoin="round" stroke-linecap="round" style="stroke:var(--layer1, #202020)"></path>
            </svg>
          </use>
        </svg>
        <h4 class="ff-base">{% spaceless %}{{-content.field_webform_phone_header[0]-}}{% endspaceless %}</h4>
        <p class="text-muted">{% spaceless %}{{-content.field_webform_phone_desc[0]-}}{% endspaceless %}</p>
        <p class="text-muted">{% spaceless %}<strong>{{-content.field_webform_phone[0]-}}{% endspaceless %}</strong></p>
      </div>
      <div class="col-md-4 text-center">
        <svg class="svg-icon svg-icon-light text-primary w-3rem h-3rem mb-3">
          <use xlink:href="#mail-1">
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 64 64" id="mail-1" width="100%" height="100%">
              <title>Mail</title>
              <desc>A line styled icon from Orion Icon Library.</desc>
              <path data-name="layer2" fill="none" stroke="#202020" stroke-miterlimit="10" d="M2 12l30 29 30-29M42 31.6L62 52M2 52l20-20.4" stroke-linejoin="round" stroke-linecap="round" style="stroke:var(--layer1, #202020)"></path>
              <path data-name="layer1" fill="none" stroke="#202020" stroke-miterlimit="10" d="M2 12h60v40H2z" stroke-linejoin="round" stroke-linecap="round" style="stroke:var(--layer1, #202020)"></path>
            </svg>
          </use>
        </svg>
        <h4 class="ff-base">{% spaceless %}{{-content.field_webform_email_header[0]-}}{% endspaceless %}</h4>
        <p class="text-muted">{% spaceless %}{{-content.field_webform_email_desc[0]-}}{% endspaceless %}</p>
        <ul class="list-unstyled text-muted">
          <li>{% spaceless %}{{-content.field_webform_email[0]-}}{% endspaceless %}</li>
        </ul>
      </div>
    </div>
    {% endif %}

  </div>


<section class="contact_form_webform">
  <section class="container">
      <section class="row">
        <section class="col-md-6 col-sm-12">{{content.field_webform_webform}}</section>
        <section class="col-md-6 col-sm-12">{{content.field_webform_iframe}}</section>
      </section>
  </section>
</section>
</section>
